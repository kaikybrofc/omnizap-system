{
  "name": "@kaikybrofc/omnizap-system",
  "version": "2.3.6",
  "description": "Sistema profissional de automação WhatsApp com tecnologia Baileys",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "keywords": [
    "whatsapp",
    "automation",
    "baileys",
    "omnizap",
    "messaging",
    "bot"
  ],
  "author": "OmniZap Team",
  "license": "MIT",
  "scripts": {
    "prepack": "npm run sync:npmignore",
    "sync:npmignore": "node -e \"const fs=require('node:fs');fs.copyFileSync('.gitignore','.npmignore');console.log('synced .npmignore from .gitignore');\"",
    "start": "node index.js",
    "dev": "node index.js",
    "db:init": "node database/init.js",
    "pm2:prod": "pm2 start ecosystem.prod.config.cjs",
    "deploy": "bash ./scripts/deploy.sh",
    "deploy:dry-run": "DEPLOY_DRY_RUN=1 bash ./scripts/deploy.sh",
    "readme:sync-snapshot": "node ./scripts/sync-readme-snapshot.mjs",
    "loadtest:stickers": "node ./scripts/sticker-catalog-loadtest.mjs",
    "worker:sticker:classification": "node ./scripts/sticker-worker-task.mjs --task-type classification_cycle",
    "worker:sticker:curation": "node ./scripts/sticker-worker-task.mjs --task-type curation_cycle",
    "worker:sticker:rebuild": "node ./scripts/sticker-worker-task.mjs --task-type rebuild_cycle",
    "seo:generate:satellites": "node ./scripts/generate-seo-satellite-pages.mjs",
    "seo:generate:satellites:phase1": "node ./scripts/generate-seo-satellite-pages.mjs --config docs/seo/satellite-pages-phase1.json --out public",
    "release": "bash ./scripts/release.sh",
    "release:minor": "RELEASE_TYPE=minor bash ./scripts/release.sh",
    "release:major": "RELEASE_TYPE=major bash ./scripts/release.sh",
    "test": "node --test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "node ./scripts/run-prettier-all.mjs --write",
    "format:check": "node ./scripts/run-prettier-all.mjs --check"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/baileys": "github:WhiskeySockets/Baileys",
    "axios": "^1.13.5",
    "canvas": "^3.2.1",
    "chalk": "^4.1.2",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "ffmpeg": "^0.0.4",
    "jimp": "^1.6.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.48",
    "mysql2": "^3.15.0",
    "node-cache": "^5.1.2",
    "openai": "^5.23.2",
    "pino": "^10.0.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "proper-lockfile": "^4.1.2",
    "qrcode-terminal": "^0.12.0",
    "stream-json": "^1.9.1",
    "uuid": "^13.0.0",
    "webp-conv": "github:kaikybrofc/webp-conv",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "pm2": "^6.0.11",
    "prettier": "^3.8.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaikybrofc/omnizap-system.git"
  },
  "bugs": {
    "url": "https://github.com/kaikybrofc/omnizap-system/issues"
  },
  "homepage": "https://github.com/kaikybrofc/omnizap-system#readme"
}
